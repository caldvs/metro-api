{
  "name": "metro-api",
  "version": "1.0.0",
  "main": "handler.js",
  "scripts": {
    "build:ts": "tsc --incremental",
    "invoke:station": "npm run-s build:ts && serverless invoke local --function station --data '{ \"pathParameters\": {\"stationId\":\"SPS\"}}'",
    "invoke:fal": "npm run-s build:ts && serverless invoke local --function firstAndLast",
    "test": "jest",
    "lint": "npx eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src test --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "deploy": "npm run-s build:ts && serverless deploy --stage dev"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "jest": "^28.1.2",
    "lodash": "^4.17.21",
    "serverless": "^3.19.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "2.7.1",
    "serverless-dotenv-plugin": "^4.0.1",
    "serverless-plugin-typescript": "^2.1.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  }
}
